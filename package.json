{
  "name": "<APP_NAME>",
  "type": "module",
  "scripts": {
    "dev": "vinxi dev",
    "build": "vinxi build",
    "start": "vinxi start",
    "deploy": "wrangler pages deploy ./dist",
    "dev:wrangler": "wrangler pages dev dist --live-reload",
    "db:start": "turso dev --db-file=local.db",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:drop": "drizzle-kit drop",
    "db:studio": "drizzle-kit studio",
    "db:pull": "drizzle-kit pull",
    "db:push": "drizzle-kit push",
    "db:seed": "tsx --env-file=.env ./src/db/seed.ts",
    "db:init": "pnpm db:generate && pnpm db:migrate && pnpm db:seed",
    "preinstall": "npx only-allow pnpm",
    "preview": "wrangler pages dev ./dist",
    "test": "vitest",
    "types": "types:check",
    "types:build": "wrangler types ./src/global.d.ts && tsc --noEmit --pretty",
    "types:check": "tsc --noEmit",
    "ui": "pnpm dlx shadcn-solid@latest",
    "ui:add": "pnpm dlx shadcn-solid@latest add"
  },
  "dependencies": {
    "@kobalte/core": "^0.13.6",
    "@kobalte/tailwindcss": "^0.9.0",
    "@kobalte/utils": "^0.9.1",
    "@libsql/client": "^0.14.0",
    "@solidjs/router": "^0.14.7",
    "@solidjs/start": "^1.0.8",
    "autoprefixer": "^10.4.19",
    "clsx": "^2.1.1",
    "cva": "1.0.0-beta.1",
    "drizzle-orm": "^0.33.0",
    "drizzle-valibot": "^0.2.0",
    "lucide-solid": "^0.446.0",
    "nanoid": "^5.0.7",
    "postcss": "^8.4.38",
    "solid-js": "^1.9.1",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.3",
    "valibot": "1.0.0-beta.0",
    "vinxi": "^0.4.3"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^22.7.4",
    "drizzle-kit": "^0.24.2",
    "solid-devtools": "^0.30.1",
    "tsx": "^4.19.1",
    "wrangler": "^3.79.0"
  }
}
